{
    "name": "really-nicca-monorepo",
    "version": "0.0.1",
    "private": true,
    "description": "Accurate Knowledge Graph Extraction Engine",
    "scripts": {
        "dev:backend": "pnpm --filter backend dev",
        "dev:ml": "echo 'Run python -m uvicorn app.main:app --reload in services/ml-service'",
        "build": "pnpm -r build",
        "test": "pnpm -r test",
        "clean": "pnpm -r exec rm -rf dist node_modules",
        "lint": "pnpm -r lint",
        "lint:fix": "pnpm -r lint:fix",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
        "type-check": "pnpm -r type-check"
    },
    "workspaces": [
        "packages/*",
        "services/*"
    ],
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    },
    "packageManager": "pnpm@8.15.4"
}